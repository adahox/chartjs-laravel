{
    "name": "olharacessivel/chartjs-laravel",
    "description": "A powerful and light library to use chartjs on Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Olharacessivel\\ChartjsLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Olharacessivel\\ChartjsLaravel\\ChartjsLaravelServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Ad√£o Dias",
            "email": "adahox@gmail.com"
        }
    ],
    "require-dev": {
        "squizlabs/php_codesniffer": "*"
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs . --ignore=\"./vendor, *.blade.php\"",
        "phpcbf": "php vendor/bin/phpcbf . --ignore=\"./vendor\""
    }
}